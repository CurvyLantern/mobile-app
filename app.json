{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/adaptive-icon.png"
      },
      "package": "com.anonymous.neonatologie"
    },
    "assetBundlePatterns": ["**/*"],
    "icon": "./assets/icon.png",
    "ios": {
      "supportsTablet": true
    },
    "name": "neonatologie",
    "orientation": "default",
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/AeonisMedium.ttf",
            "./assets/fonts/AeonisBold.ttf",
            "./assets/fonts/AeonisBoldExtended.ttf",
            "./assets/fonts/AeonisExtended.ttf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "slug": "neonatologie",
    "splash": {
      "backgroundColor": "#333333",
      "image": "./assets/splash-logo-full.png",
      "resizeMode": "contain"
    },
    "userInterfaceStyle": "light",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.ico"
    }
  }
}
